{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "noEmit": false,
    "outDir": "./",
    "rootDir": "./",
    "module": "Node16",
    "moduleResolution": "node16",
    "allowImportingTsExtensions": false,
    "verbatimModuleSyntax": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["api/**/*"],
  "exclude": ["node_modules", "dist", "src"]
}

